<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/NM_Date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:inputType="none" android:text="date"/>
    <TextView
        android:id="@+id/NM_Heure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/NM_Douleur"
        android:inputType="none" android:text="heure"/>
    <TextView
        android:id="@+id/NM_Douleur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:inputType="none"
        android:text="douleur"/>

    <TextView
        android:id="@+id/NM_Medic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/NM_Heure"
        android:layout_toEndOf="@+id/NM_Douleur" android:inputType="none"
        android:text="medicament"/>
    <TextView
        android:id="@+id/NM_Dose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/NM_Nom"
        android:layout_toEndOf="@+id/NM_Medic" android:inputType="none"
        android:text="dose"/>
    <TextView
        android:id="@+id/NM_Nom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/NM_Dose"
        android:inputType="none" android:text="nom"/>

</RelativeLayout>